{
    "name": "kadirov/role-permission",
    "description": "Laravel Role Permission paket",
    "type": "project",
 
    "repositories": [
        {
            "type": "path",
            "url": "../laravel-role-permission"
        }
    ],

    "autoload": {
        "psr-4": {
            "Kadirov\\RolePermission\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Kadirov\\RolePermission\\RolePermissionServiceProvider"
            ]
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },

    "authors": [
        {
            "name": "Abdug'offor",
            "email": "abdugofforqodirov19@gmail.com"
        }
    ]
}
